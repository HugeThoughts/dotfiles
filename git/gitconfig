[credential]
	helper = cache --timeout=99999

# push easily. http://stackoverflow.com/a/23918418/89484
[push]
	default = current

[color]
	ui = true
[alias]
	co = checkout
	br = branch
	ct = commit
	unstage = reset HEAD --
	undo = reset --soft HEAD^
[url "git@github.com:HugeThoughts/"]
  insteadOf = "git://github.com/HugeThoughts/"
